{
  "name": "code",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.7",
    "express": "^4.21.1",
    "firebase": "^11.0.2",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "ts-node src/components/server.ts",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --no-watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.0",
    "fetch-mock": "^12.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5",
    "whatwg-fetch": "^3.6.20"
  },
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
